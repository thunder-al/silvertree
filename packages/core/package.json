{
  "name": "@silvertree/core",
  "version": "0.0.3",
  "keywords": [
    "silvertree",
    "di",
    "ioc",
    "container"
  ],
  "author": "ThunderAl <community@thornal.com>",
  "license": "MIT",
  "description": "Core package for Silvertree framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/thunder-al/silvertree.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/thunder-al/silvertree",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest",
    "watch": "vite build -w -m development",
    "dev": "vite build -m development",
    "build": "vite build -m production"
  },
  "devDependencies": {
    "vitest": "~0.34.6",
    "vite": "~5.0.2",
    "vite-plugin-dts": "~3.6.3",
    "vite-plugin-checker": "~0.6.2",
    "typescript": "~5.2.2",
    "reflect-metadata": "~0.1.13",
    "@types/nested-error-stacks": "~2.1.1"
  },
  "peerDependencies": {
    "reflect-metadata": "~0.1.13"
  },
  "dependencies": {
    "nested-error-stacks": "~2.1.1"
  }
}
