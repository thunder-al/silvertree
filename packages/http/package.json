{
  "name": "@silvertree/http",
  "version": "0.0.2",
  "keywords": [
    "silvertree",
    "http",
    "fastify"
  ],
  "author": "ThunderAl <community@thornal.com>",
  "license": "MIT",
  "description": "Http package for Silvertree framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/thunder-al/silvertree.git",
    "directory": "packages/http"
  },
  "homepage": "https://github.com/thunder-al/silvertree",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "vitest --threads=false --single-thread --run",
    "test:watch": "vitest --threads=false --single-thread",
    "watch": "vite build -w -m development",
    "dev": "vite build -m development",
    "build": "vite build -m production",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "vitest": "~0.34.6",
    "vite": "~5.0.2",
    "vite-plugin-dts": "~3.6.3",
    "vite-plugin-checker": "~0.6.2",
    "typescript": "~5.2.2",
    "reflect-metadata": "~0.1.13",
    "@silvertree/logging": "workspace:*",
    "@silvertree/core": "workspace:*"
  },
  "peerDependencies": {
    "reflect-metadata": "~0.1.13",
    "@silvertree/logging": "workspace:*",
    "@silvertree/core": "workspace:*"
  },
  "dependencies": {
    "@fastify/error": "~3.4.1",
    "@fastify/formbody": "~7.4.0",
    "@fastify/multipart": "~8.0.0",
    "fastify": "~4.24.3",
    "@fastify/accepts": "~4.3.0",
    "@fastify/cookie": "~9.2.0",
    "@fastify/cors": "~8.4.2",
    "@fastify/routes": "~5.1.0"
  }
}
