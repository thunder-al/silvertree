  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thunder-al/silvertree.git",
    "directory": "documentation"
  },
  "scripts": {
    "dev": "vitepress dev",
    "build": "npm run build:api && npm run build:doc",
    "build:doc": "vitepress build 2>&1",
    "preview": "vitepress preview",
    "build:api": "typedoc --options ../typedoc.json --tsconfig ../tsconfig.base.json --out ./public/api 2>&1"
  },
  "devDependencies": {
    "vitepress": "~1.0.0-rc.31"
  },
  "dependencies": {
    "mermaid": "~10.6.1",
    "vue": "~3.4.7",
    "markdown-it-container": "~4.0.0",
    "typedoc": "~0.25.7"
  }
}
